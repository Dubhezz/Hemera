/**
* generate from Hemera
* https://github.com/JianweiWangs/Hemera.git
* JianweiWangs
* <%=@class_name%>.m
*/

<%= "#import \"#{@class_name}.h\"\n" %>

<%= "@implementation #{@class_name}\n" %>

<% for @image in @images %>
  <%= "+ (#{@class_name})#{@image.name} {\n"%>
  <%= "\t#{@class_name} *model = [#{@class_name} new];\n" %>
  <%= "\tmodel.light = [UIImage imageNamed:@\"#{@image.name_light}\";\n" %>
  <%= "\tmodel.dark = [UIImage imageNamed:@\"#{@image.name_dark}\";\n" %>
  <%= "\treturn model;" %>
  <%= "}\n" %>
<% end %>
@end

